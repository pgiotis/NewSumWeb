<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0" packagerversion="1.5.0a1">
 <name>wsdl2php</name>
 <summary>wsdl2php is a tool for generating proxy classes that can be used to access web services</summary>
 <description>PHP 5 Web Service Proxy Generator
 </description>
 <maintainers>
  <maintainer>
   <user>knut</user>
   <name>Knut Urdalen</name>
   <email>knut.urdalen@gmail.com</email>
   <role>lead</role>
  </maintainer>
  </maintainers>
 <release>
  <version>0.2.1</version>
  <date>2007-01-03</date>
  <license>LGPL</license>
  <state>alpha</state>
  <notes>wsdl2php is a simple tool for PHP 5 to generate client code against a WSDL-file
  </notes>
  <deps>
   <dep type="php" rel="ge" version="5.0.1"/>
  </deps>
  <provides type="function" name="parse_doc" />
  <provides type="function" name="checkForEnum" />
  <provides type="function" name="findType" />
  <provides type="function" name="generatePHPSymbol" />
  <filelist>
   <dir name="bin">
    <file role="script" baseinstalldir="/" platform="(*ix|*ux|darwin*|SunOS*)" install-as="wsdl2php" name="wsdl2php">
     <replace type="pear-config" from="@PHP-BIN@" to="php_bin"/>
     <replace type="pear-config" from="@BIN-DIR@" to="bin_dir"/>
     <replace type="pear-config" from="@PEAR-DIR@" to="php_dir"/>
    </file>
    <file role="php" baseinstalldir="/" install-as="wsdl2php.php" name="wsdl2php.php"/>
   </dir> <!-- /bin -->
  </filelist>
 </release>
 <changelog>
   <release>
    <version>0.2.1</version>
    <date>2007-01-03</date>
    <license>LGPL</license>
    <state>alpha</state>
    <notes>wsdl2php is a simple tool for PHP 5 to generate client code against a WSDL-file
    </notes>
   </release>
 </changelog>
</package>
